@model ATOMv0.Models.DimModule
@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm("Edit", "DimModules", FormMethod.Post, new { @class = "form-crud" }))
{
    @Html.AntiForgeryToken()

    <div>
        <h2>Edit</h2>
        <h4>DimModule</h4>
        <hr />
        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.id)

        <div class="form-group">
            @Html.LabelFor(model => model.ModuleName, htmlAttributes: new { @class = "control-label" })
            <div>
                @Html.EditorFor(model => model.ModuleName)
                @Html.ValidationMessageFor(model => model.ModuleName)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.KeyBuilding, "KeyBuilding", htmlAttributes: new { @class = "control-label" })
            <div>
                @Html.DropDownList("KeyBuilding", String.Empty)
                @Html.ValidationMessageFor(model => model.KeyBuilding)
                <br /><br />
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-1 col-md-10">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </div>
    </div>

    <div>
        @Html.ActionLink("Back to List", "Index")
    </div>

}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
